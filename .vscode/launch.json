    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: run.py train_debug.csv",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": [
                "--gpu",
                "0",
                "--training_list",
                "execute_list/train_debug.csv",
                "--max_token_num",
                "17",
                "--percent",
                "10",
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
        },
    ]
}